<%# Nested inside a DM index table %>
<tr id="<%= character[:name] %>" class="character">
  <td class="status">&nbsp;</td>
  <td class="initiative"><%= character[:initiative] %></td>
  <td class="name"><%= character[:name] %></td>
  <td class="health"><%= character[:health] %></td>
  <td class="turn <%= character[:role] %>">&nbsp;</td>
  <td><img src="../img/target.png" class="target" alt="Target Character" /></td>
</tr>
<%# End Nested inside a DM index %>
